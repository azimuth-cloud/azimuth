<?xml version="1.0" encoding="UTF-8"?>
<NatRule xmlns="http://www.vmware.com/vcloud/v1.5">
    <RuleType>DNAT</RuleType>
    <IsEnabled>true</IsEnabled>
    <GatewayNatRule>
        <Interface type="application/vnd.vmware.admin.network+xml" href="{{ network.href }}" name="{{ network.name }}" />
        <OriginalIp>{{ external_ip }}</OriginalIp>
        <OriginalPort>any</OriginalPort>
        <TranslatedIp>{{ internal_ip }}</TranslatedIp>
        <TranslatedPort>any</TranslatedPort>
        <Protocol>any</Protocol>
    </GatewayNatRule>
</NatRule>