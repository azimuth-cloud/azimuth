          xsi:schemaLocation="http://www.vmware.com/vcloud/v1.5 http://vcloud-ref.ceda.ac.uk/api/v1.5/schema/master.xsd">
    <MetadataEntry>
        <Key>JASMIN.HOST_TYPE</Key>
        <TypedValue xsi:type="MetadataStringValue">
            <Value>{{ host_type }}</Value>
        </TypedValue>
    </MetadataEntry>
    <MetadataEntry>
        <Key>JASMIN.NAT_POLICY</Key>
        <TypedValue xsi:type="MetadataStringValue">
            <Value>{{ nat_policy }}</Value>
        </TypedValue>
    </MetadataEntry>
